        "QT_QUICK_CONTROLS_STYLE": "Basic",
        "PYTHONIOENCODING": "utf-8",
        "PYTHONUTF8": "1",
        "LC_ALL": "ru-RU",
        "LANG": "ru-RU",
        "PIP_DISABLE_PIP_VERSION_CHECK": "1",
        "DOTNET_SYSTEM_GLOBALIZATION_INVARIANT": "0",
