    "PneumoStabSim (App)": {
      "commandName": "Executable",
      "executablePath": ".\\.venv\\Scripts\\python.exe",
      "commandLineArgs": "app.py",
      "workingDirectory": ".",
      "environmentVariables": {
        "PYTHONPATH": "$(MSBuildProjectDirectory);$(MSBuildProjectDirectory)\\src",
